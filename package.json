{
  "name": "metamask",
  "version": "0.1.0",
  "private": true,
  "author": "maieutiquer",
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "node": ">=18.10.0",
    "npm": ">=8.19.2"
  },
  "scripts": {
    "start": "cross-env TSC_COMPILE_ON_ERROR=true DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eslint:check": "eslint --ext .js,.jsx,.ts,.tsx . --report-unused-disable-directives",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "stylelint:check": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "prettier:check": "prettier . --ignore-unknown --check",
    "prettier:fix": "prettier . --ignore-unknown --write",
    "tsc:check": "tsc --noEmit",
    "tsc:fix": "tsc --noEmit",
    "test-related": "cross-env NODE_ENV=test CI=true react-scripts test --bail --findRelatedTests",
    "ganache": "ganache --wallet.seed myCustomSeed",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@fontsource/roboto": "4.5.8",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.8",
    "@reduxjs/toolkit": "1.8.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "cross-env": "7.0.3",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "ganache": "7.4.4",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.4",
    "react-scripts": "5.0.1",
    "sass": "1.55.0",
    "stylelint": "14.13.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-csstree-validator": "2.0.0",
    "typescript": "4.8.4",
    "web-vitals": "3.0.3"
  },
  "packageManager": "yarn@3.2.4"
}
